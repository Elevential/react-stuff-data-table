{
  "name": "react-user-data-table",
  "version": "0.0.1",
  "description": "A react component to show user data in table formart",
  "main": "build/index.js",
  "peerDependencies": {
    "react": "^15.5.4"
  },
  "dependencies": {
    "bootstrap": "^4.1.0",
    "css-loader": "^0.28.11",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "react": "^16.3.2",
    "shortid": "^2.2.8",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.4",
    "webpack": "^2.6.1"
  },
  "scripts": {
    "test": "jest --watch",
    "start": "webpack --watch",
    "build": "webpack"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js",
      "^.+\\.jsx?$": "./node_modules/babel-jest",
      "^.+\\.js?$": "./node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": {
    "name": "Yago Luis Azedias de Melo",
    "email": "yagoazedias@gmail.com"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "react-test-renderer": "^16.3.2",
    "webpack-cli": "^2.0.14"
  }
}
